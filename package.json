{
  "name": "alias-game",
  "version": "1.0.0",
  "description": "## Overview",
  "main": "server.js",
  "author": "",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "dev": "node --watch --env-file=.env.exemple src/server.js",
    "dev:debug": "node --inspect --env-file=.env src/server.js",
    "dc:up": "docker compose up -d && docker compose logs -f app",
    "dc:down": "docker compose down --remove-orphans",
    "dc:start": "docker compose start",
    "dc:stop": "docker compose stop",
    "dc:up:actions": "docker compose up -d",
    "setup:prisma": "yarn prisma generate && npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "lint": "npx eslint ."
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "prisma": "^5.20.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@prisma/client": "^5.20.0",
    "bcryptjs": "^2.4.3",
    "eslint": "^9.11.1",
    "globals": "^15.9.0"
  }
}
